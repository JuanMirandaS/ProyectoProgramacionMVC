@model ProyectoProgramacionMVC.Models.Marca

@{
    ViewData["Title"] = "Details";
}

<h1>Detalles de la marca</h1>

<div>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Nombre)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Nombre)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model?.MarcaId" class="btn btn-primary">Editar marca</a> |
    <a asp-action="Index" class="btn btn-primary">Volver atras</a>
</div>
