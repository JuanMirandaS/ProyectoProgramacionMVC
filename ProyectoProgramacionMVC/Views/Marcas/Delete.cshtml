@model ProyectoProgramacionMVC.Models.Marca

@{
    ViewData["Title"] = "Delete";
}

<h1>Eliminar marca</h1>
<hr />
<h3>Estas seguro que deseas eliminar esta marca?</h3>
<div>
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Nombre)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Nombre)
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="MarcaId" />
        <input type="submit" value="Eliminar marca" class="btn btn-danger" /> |
        <a asp-action="Index" class="btn btn-primary">Volver atras</a>
    </form>
</div>
